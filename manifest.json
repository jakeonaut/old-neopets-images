{
  "name": "Old Neopets Images",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Use this extension to get the old neopet pictures back. Currently only works for pets. This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://cakeandturtles.nfshost.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.neopets.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "web_accessible_resources": [ "src/inject/oldNeopetsImageScript.js"],
  "permissions": [
	"http://*/"
  ]
}